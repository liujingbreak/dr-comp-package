{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src/internal/ng-app-builder/src",
  "projects": {
    "ng-app-builder": {
      "projectType": "application",
      "root": ".",
      "sourceRoot": "src/internal/ng-app-builder/src",
      "prefix": "drcp",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "spec": false,
      "prefix": "cc",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "spec": false,
      "prefix": "cc"
    },
    "@schematics/angular:guard": {
      "spec": false
    },
    "@schematics/angular:module": {
      "spec": false
    },
    "@schematics/angular:pipe": {
      "spec": false
    },
    "@schematics/angular:service": {
      "spec": false
    }
  },
  "defaultProject": "ng-app-builder"
}

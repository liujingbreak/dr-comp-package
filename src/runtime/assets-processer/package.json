{
  "name": "@dr-core/assets-processer",
  "version": "1.1.29",
  "description": "Copy assets to destDir folder only when devMode is false",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dr": {
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": 8000,
    "serverPriority": 1000,
    "category": ["internal", "nodejs", "tool"],
    "config": {
      "server": {
        "fetchUrl": null,
        "fetchRetry": 5,
        "downloadMode": "fork",
        "fetchLogErrPerTimes": 20,
        "fetchIntervalSec": 90,
        "cacheControlMaxAge": {
          "js": "365 days",
          "css": "365 days",
          "less": "365 days",
          "html": null,
          "png": "365 days",
          "jpg": "365 days",
          "jpeg": "365 days",
          "gif": "365 days",
          "svg": "365 days",
          "eot": "365 days",
          "ttf": "365 days",
          "woff": "365 days",
          "woff2": "365 days"
        },
        "fallbackIndexHtml": {
          "^/[^/]+": "<%=match[0]%>/index.html"
        },
        "fetchMailServer": {
          "imap": "A.imap.com",
          "smtp": "B.smtp.com",
          "user": "somebody",
          "loginSecret": "fake one"
        },
        "serveIndex": false,
        "requireToken": false
      }
    },
    "config.local": {
      "server": {
        "fetchRetry": 0,
        "fetchLogErrPerTimes": 1,
        "fetchIntervalSec": 60,
        "cacheControlMaxAge": {
        },
        "fetchMailServer": null,
        "indexHtmlProxy": {
          "target": "http://localhost:4200"
        }
      }
    }
  },
  "--dep-for-charset-transform": {
    "@types/iconv": "~2.1.16",
    "iconv": "~2.3.5"
  },
  "dependencies": {
    "request": "2.88.0",
    "@types/request": "~2.48.3",
    "through2": "^2.0.3",
    "shelljs": "^0.7.8",
    "mkdirp": "^0.5.1",
    "serve-favicon": "^2.4.4",
    "gulp-zip": "4.1.0",
    "gulp-unzip": "1.0.0",
    "adm-zip": "^0.4.13",
    "@types/adm-zip": "^0.4.32",
    "serve-static-zip": "^1.0.3",
    "pify": "~4.0.1",
    "http-proxy-middleware": "^0.19.1",
    "@types/through2": "^2.0.34",
    "nodemailer": "^6.3.0",
    "@types/nodemailer": "^6.2.1",
    "uuid-random": "~1.3.0",
    "@types/serve-index": "^1.7.30",
    "serve-index": "^1.9.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}

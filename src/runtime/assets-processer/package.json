{
  "name": "@dr-core/assets-processer",
  "version": "1.0.14",
  "description": "Copy assets to destDir folder only when devMode is false",
  "main": "assetsProcesser.js",
  "dr": {
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": 8000,
    "serverPriority": 1000,
    "category": ["internal", "nodejs", "tool"],
    "config": {
      "server": {
        "fetchUrl": null,
        "fetchRetry": 3,
        "fetchLogErrPerTimes": 20,
        "fetchIntervalSec": 300
      }
    },
    "config.local": {
      "server": {
        "fetchRetry": 0,
        "fetchLogErrPerTimes": 1,
        "fetchIntervalSec": 30
      }
    }
  },
  "dependencies": {
    "request": "2.88.0",
    "@types/request": "2.47.1",
    "through2": "^2.0.3",
    "shelljs": "^0.7.8",
    "mkdirp": "^0.5.1",
    "serve-favicon": "^2.4.4",
    "gulp-zip": "4.1.0",
    "gulp-unzip": "1.0.0",
    "adm-zip": "^0.4.11"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}

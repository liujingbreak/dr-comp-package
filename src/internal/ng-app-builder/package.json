{
  "name": "@dr-core/ng-app-builder",
  "version": "2.1.16",
  "description": "Angular 5.0 App build based on skeleton generated by @angular/cli",
  "browser": "browser.ts",
  "main": "dist/server.js",
  "builders": "ts/build-angular/builders.json",
  "schematics": "./ts/ng/schematics/collection.json",
  "dr": {
    "category": ["tool", "internal", "nodejs"],
    "chunk": "common-app",
    "entryPage": "index.html",
    "angularCompiler": true,
    "type": ["builder", "server"],
    "builderPriority": 9999,
    "ngTsHandler": "dist/server#tsHandler",
    "config": {
      "server": {
        "outputPath": "dist/static",
        "tsconfigInclude": [],
        "ng8Compliant": true
      }
    }
  },
  "peerDependencies": {
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@angular/platform-server": "~8.0.3",
    "@types/karma": "^3.0.3",
    "@angularclass/hmr": "^2.1.3"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/request": "^2.47.1",
    "@types/glob": "^7.1.1",
    "@types/semver": "^5.3.30",
    "@types/webpack": "^4.4.12",
    "cssnano": "^3.10.0",
    "webpack-hot-middleware": "2.22.1",
    "@types/webpack-dev-server": "2.9.6",
    "fs-extra": "^5.0.0",
    "gzip-size": "^4.1.0",
    "pretty-bytes": "^5.0.0",
    "loader-utils": "^1.1.0",
    "@types/loader-utils": "^1.1.3",
    "reflect-metadata": "^0.1.12",
    "domino": "2.0.3",
    "resolve-url-loader": "^2.3.0",
    "url-loader": "^2.1.0",
    "comment-json": "^1.1.3",
    "html-loader": "^0.5.5",
    "raw-loader": "^0.5.1",
    "yaml-loader": "^0.5.0",
    "json-loader": "^0.5.7"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}

{
  "name": "@dr-core/ng-app-builder",
  "version": "2.0.50",
  "description": "Angular 5.0 App build based on skeleton generated by @angular/cli",
  "browser": "browser.ts",
  "main": "dist/server.js",
  "builders": "ng-schemas/builders.json",
  "schematics": "./ts/ng/schematics/collection.json",
  "dr": {
    "category": ["tool", "internal", "nodejs"],
    "chunk": "common-app",
    "entryPage": "index.html",
    "angularCompiler": true,
    "type": ["builder", "server"],
    "builderPriority": 9999,
    "ngTsHandler": "dist/server#tsHandler",
    "config": {
      "server": {
        "ngModule": ["@dr-core/ng-app-builder/src/app#DeveloperModule"],
        "ngPackage": [],
        "deployUrl": "/",
        "outputPath": "dist/static",
        "excludePackage": [],
        "excludePath": [],
        "build-optimizer:exclude": []
      }
    }
  },
  "peerDependencies": {
    "@angular-devkit/core": "7.0.3",
    "@angular-devkit/schematics": "7.0.3",
    "@ngtools/webpack": "7.0.3",
    "webpack": "4.19.1",
    "@angular-devkit/build-angular": "0.10.3",
    "@angular/cli": "7.0.3",
    "@angular/compiler-cli": "7.0.1",
    "@angular/language-service": "7.0.1",
    "@nguniversal/express-engine": "7.0.2",
    "@angularclass/hmr": "^2.1.3",
    "@angular/platform-server": "^7.0.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/request": "^2.47.0",
    "@types/glob": "^5.0.29",
    "@types/istanbul": "^0.4.29",
    "@types/semver": "^5.3.30",
    "@types/webpack": "^4.4.12",
    "cssnano": "^3.10.0",
    "webpack-hot-middleware": "2.22.1",
    "@types/webpack-dev-server": "2.9.6",
    "gzip-size": "^4.1.0",
    "pretty-bytes": "^5.0.0",
    "postcss": "^6.0.22",
    "loader-utils": "^1.1.0",
    "source-map": "^0.6.1",
    "@types/loader-utils": "^1.1.3",
    "reflect-metadata": "^0.1.12",
    "zone.js": "^0.8.26",
    "domino": "2.0.3",
    "resolve-url-loader": "^2.3.0",
    "comment-json": "^1.1.3"
  },
  "back-dep": {
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}

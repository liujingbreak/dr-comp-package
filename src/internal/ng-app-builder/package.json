{
  "name": "@dr-core/ng-app-builder",
  "version": "2.0.26",
  "description": "Angular 5.0 App build based on skeleton generated by @angular/cli",
  "browser": "browser.ts",
  "main": "dist/server.js",
  "builders": "ng-schemas/builders.json",
  "schematics": "./ts/ng/schematics/collection.json",
  "dr": {
    "category": ["tool", "internal", "nodejs"],
    "chunk": "common-app",
    "entryPage": "index.html",
    "angularCompiler": true,
    "type": ["builder", "server"],
    "builderPriority": 9999,
    "config": {
      "server": {
        "ngModule": [],
        "ngPackage": [],
        "deployUrl": "/",
        "outputPath": "dist/static",
        "excludePackage": [],
        "excludePath": []
      }
    }
  },
  "peerDependencies": {
    "@angular-devkit/core": "0.7.4",
    "@angular-devkit/schematics": "0.7.4",
    "webpack": "~4.9.2",
    "@angular-devkit/build-angular": "0.7.4",
    "@angular/cli": "6.1.4",
    "@angular/compiler-cli": "6.1.3",
    "@angular/language-service": "6.1.3",
    "@nguniversal/express-engine": "6.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@angular/platform-server": "6.1.3",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@angular/platform-browser": "6.1.3",
    "@angular/platform-browser-dynamic": "6.1.3"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/request": "^2.47.0",
    "@types/glob": "^5.0.29",
    "@types/istanbul": "^0.4.29",
    "@types/semver": "^5.3.30",
    "@types/webpack": "^4.4.10",
    "cssnano": "^3.10.0",
    "webpack-hot-middleware": "2.22.1",
    "@types/webpack-dev-server": "2.9.6",
    "@angularclass/hmr": "^2.1.3",
    "gzip-size": "^4.1.0",
    "pretty-bytes": "^5.0.0",
    "postcss": "^6.0.22",
    "loader-utils": "^1.1.0",
    "source-map": "^0.6.1",
    "@types/loader-utils": "^1.1.3",
    "reflect-metadata": "^0.1.12",
    "zone.js": "^0.8.26",
    "domino": "2.0.3",
    "resolve-url-loader": "^2.3.0"
  },
  "back-dep": {
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },

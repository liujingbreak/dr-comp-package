{
    "$schema": "../../../../../node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
      "drcp-default-app": {
        "root": "node_modules/@dr-core/ng-app-builder",
        "sourceRoot": "node_modules/@dr-core/ng-app-builder/src",
        "projectType": "application",
        "prefix": "app",
        "schematics": {
          "@schematics/angular:component": {
            "styleext": "less"
          }
        },
        "architect": {
          "build": {
            "builder": "@angular-devkit/build-angular:browser",
            "options": {
              "outputPath": "dist/static",
              "index": "index.html",
              "main": "node_modules/@dr-core/ng-app-builder/src/main.ts",
              "polyfills": "node_modules/@dr-core/ng-app-builder/src/polyfills.ts",
              "tsConfig": "node_modules/@dr-core/ng-app-builder/src/tsconfig.app.json",
              "assets": [
              ],
              "styles": [
                "node_modules/@dr-core/ng-app-builder/src/styles.less"
              ],
              "scripts": [],
              "aot": true
            },
            "configurations": {
              "production": {
                "fileReplacements": [
                  {
                    "replace": "node_modules/@dr-core/ng-app-builder/src/environments/environment.ts",
                    "with": "node_modules/@dr-core/ng-app-builder/src/environments/environment.prod.ts"
                  }
                ],
                "optimization": true,
                "outputHashing": "all",
                "sourceMap": false,
                "extractCss": true,
                "namedChunks": false,
                "aot": true,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true
              }
            }
          },
          "serve": {
            "builder": "@dr-core/ng-app-builder:dev-server",
            "options": {
              "browserTarget": "drcp-default-app:build",
              "hmr": true
            },
            "configurations": {
              "production": {
                "browserTarget": "drcp-default-app:build:production"
              }
            }
          },
          "extract-i18n": {
            "builder": "@angular-devkit/build-angular:extract-i18n",
            "options": {
              "browserTarget": "drcp-default-app:build"
            }
          },
          "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
              "main": "node_modules/@dr-core/ng-app-builder/src/test.ts",
              "polyfills": "node_modules/@dr-core/ng-app-builder/src/polyfills.ts",
              "tsConfig": "node_modules/@dr-core/ng-app-builder/src/tsconfig.spec.json",
              "karmaConfig": "node_modules/@dr-core/ng-app-builder/src/karma.conf.js",
              "styles": [
                "node_modules/@dr-core/ng-app-builder/styles.less"
              ],
              "scripts": [],
              "assets": [
                "node_modules/@dr-core/ng-app-builder/src/favicon.ico",
                "node_modules/@dr-core/ng-app-builder/src/assets"
              ]
            }
          },
          "lint": {
            "builder": "@angular-devkit/build-angular:tslint",
            "options": {
              "tsConfig": [
                "node_modules/@dr-core/ng-app-builder/src/tsconfig.app.json",
                "node_modules/@dr-core/ng-app-builder/src/tsconfig.spec.json"
              ],
              "exclude": [
                "**/node_modules/**"
              ]
            }
          }
        }
      }
    },
    "defaultProject": "drcp-default-app"
  }
    "pify": "^3.0.0",
    "react-hot-loader": "^4.1.1",
    "sass-loader": "^7.0.1",
    "jade": "^1.11.0",
    "promise-polyfill": "^7.1.2",
    "manual-chunk-plugin": "^2.0.0",
    "webpack-bundle-analyzer": "^2.12.0",
    "ts-loader": "4.2.0",
    "core-js": "2.5.3",
    "es5-shim": "^4.5.10",
    "uglifyjs-webpack-plugin": "1.1.8",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "happypack": "^4.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "babel-plugin-lodash": "^3.3.2",
    "license-webpack-plugin": "^1.3.0"
  },
  "author": "LJ",
  "license": "MIT",
  "dr": {
    "category": ["tool", "internal", "nodejs"],
    "chunk": "common-app",
    "cssScope": false,
    "type": [
      "builder",
      "server"
    ],
    "jsLoader": "babel",
    "builderPriority": 0,
    "config": {
      "server": {
        "defaultChunkName": "extra-chunk",
        "fileNameSuffix": "",
        "inlineCssSize": 2048,
        "inlineJsSize": 2048,
        "symlinks": true,
        "useHappypack": false,
        "happyThreadPoolSize": 2,
        "tsChecker": {
          "memoryLimit": 1048
        },
        "no-require-lodash-loader": false
      }
    },
    "config.local": {
      "server": {
        "defaultChunkName": "extra-chunk",
        "fileNameSuffix": "",
        "inlineCssSize": 2048,
        "inlineJsSize": 0,
        "symlinks": true,
        "useHappypack": false,
        "happyThreadPoolSize": 2,
        "tsChecker": {
          "memoryLimit": 512
        },
        "no-require-lodash-loader": false
      }
    }
  }
}

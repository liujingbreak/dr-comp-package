{
  "name": "@dr/translate-generator",
  "version": "1.0.8",
  "description": "A tool to scan source package files and generate translatable files",
  "main": "translate-generator.js",
  "loader": "loader",
  "dr": {
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": "before @dr-core/webpack2-builder",
    "config": {
      "server": {
        "scanMethodNames": [
          "drTranslate"
        ]
      }
    }
  },
  "dependencies": {
    "acorn": "^5.3.0",
    "acorn-jsx": "^4.1.0",
    "acorn-dynamic-import": "^3.0.0",
    "estraverse-fb": "^1.3.2",
    "estraverse": "^4.2.0",
    "cheerio": "^0.22.0",
    "mkdirp": "^0.5.1",
    "js-yaml": "^3.10.0",
    "browser-resolve": "^1.11.2",
    "pify": "^3.0.0",
    "glob": "^7.1.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "ISC"
}

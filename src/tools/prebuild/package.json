{
  "name": "@wfh/prebuild",
  "version": "1.0.27",
  "description": "Work with Plink, prebuild command line tool for monorepo based project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dr": {
    "cli": "dist/cli.js#default",
    "config": {
      "server": {
        "prebuildGitRemote": "origin",
        "prebuildReleaseBranch": "release-server",
        "installEndpoint": {
          "local": "http://localhost:14333/_install"
        }
      }
    },
    "config.local": {
      "server": {
        "prebuildReleaseBranch": "release-server-local"
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "commander": "~5.1.0",
    "boxen": "~4.2.0",
    "adm-zip": "^0.4.14",
    "@types/adm-zip": "^0.4.33",
    "yazl": "2.5.1",
    "@types/yazl": "2.4.2",
    "yauzl": "~2.10.0",
    "@types/yauzl": "~2.9.1"
  },
  "author": "LJ",
  "license": "MIT"
}
